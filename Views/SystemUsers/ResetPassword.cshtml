@model SystemUser
<h4>ResetPassword</h4>
<input type="hidden" value="@ViewBag.passwordSend" id="succ" />
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="ResetPassword">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" name="id" id="id" value="@Context.Session.GetInt32("Id")"/>
            <div class="form-group">
                <label asp-for="Password" class="control-label"></label>
                <input id="password"name="password" type="password" class="form-control" />
            </div>
            @*<div class="form-group">
                <label for="CofirmPassword" class="control-label"></label>
                <input id="CPassword" name="CPassword" type="password" class="form-control " />
            </div>*@
            <div class="form-group">
                <input type="submit" value="Submit" class="btn btn-primary submit" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    <script>
        $(function () {

            if ($('#succ').val() != null && $('#succ').val() != '' && $('#succ').val() != 'undefined') {
                swal("Congratulations!", $('#succ').val(), "success");
            }
                //if ($("#CPassword").val() != $("#Password").val()) {
                //    swal("Your Password is Mismatch", "error");
                //} 
           

        });
    </script>

}



